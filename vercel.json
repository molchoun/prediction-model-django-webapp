{
    "builds": [
      {
        "src": "prediction_project/prediction_project/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "prediction_project/static"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "prediction_project/prediction_project/wsgi.py"
      }
    ]
}